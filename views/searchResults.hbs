<html>
  <head>
    <title>Search Results</title>
    <style>
      body { font-family: Arial, sans-serif; line-height: 1.6; margin: 20px; }
      h1 { text-align: center; } table { width: 100%; border-collapse: collapse;
      margin-top: 20px; } th, td { border: 1px solid #ddd; padding: 8px;
      text-align: left; } th { background-color: #f2f2f2; } tbody
      tr:nth-child(even) { background-color: #f9f9f9; } img { max-width: 100px;
      max-height: 100px; } strong { margin-right: 5px; font-weight: bold; } p {
      text-align: center; }
    </style>
  </head>
  <body>
    <h1>Search Results</h1>
    <div>
      {{#if searchResult.length}}
        <table>
          <thead>
            <tr>
              <th>Invoice ID</th>
              <th>Branch</th>
              <th>City</th>

              <th>Image</th>
              <th>Quantity</th>
              <th>Total</th>
              <th>Date</th>
              <th>Time</th>
              <th>Payment</th>
              <th>COGS</th>
              <th>Rating</th>
            </tr>
          </thead>
          <tbody>
            {{#each searchResult}}
              <tr>
                <td>{{getProperty this "Invoice ID"}}</td>
                <td>{{getProperty this "Branch"}}</td>
                <td>{{getProperty this "City"}}</td>
                <td>
                  <img
                    src="{{getProperty this 'image'}}"
                    alt="{{getProperty this 'name'}}"
                    onerror="this.src='data:image/png;base64,';
                                   this.alt='Image Error'"
                  />
                </td>
                <td>{{getProperty this "Quantity"}}</td>
                <td>{{getProperty this "Total"}}</td>
                <td>{{getProperty this "Date"}}</td>
                <td>{{getProperty this "Time"}}</td>
                <td>{{getProperty this "Payment"}}</td>
                <td>{{getProperty this "COGS"}}</td>
                <td>{{getProperty this "Rating"}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      {{else}}
        <p>No results found for the provided Invoice ID.</p>
      {{/if}}
    </div>
  </body>
</html>

